<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/F3F3F3"
    android:orientation="vertical" >

    <include layout="@layout/title_back" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:gravity="center"
        android:text="您的注册手机号码"
        android:textColor="@color/black"
        android:textSize="@dimen/s_17sp" />

    <TextView
        android:id="@+id/phoneText"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="60dp"
        android:layout_marginRight="60dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="20dp"
        android:gravity="center"
        android:textColor="@color/bgtitle"
        android:textSize="@dimen/s_17sp" />

    <View
        android:layout_width="fill_parent"
        android:layout_height="0.5dp"
        android:background="@color/adadad" />

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="@dimen/d_48dp"
        android:background="@color/ffffff" >


        <TextView
            android:id="@+id/login_img_lock1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:text="填写登录密码"
            android:textColor="@color/text292929"
            android:textSize="17sp" />

        <EditText
            android:id="@+id/login_edit_pass"
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_toRightOf="@id/login_img_lock1"
            android:background="@color/ffffff"
            android:digits="0123456789qwertyuioplkjhgfdsazxcvbnm"
            android:hint="密码由6~20位数字与字母组成"
            android:inputType="textPassword"
            android:maxWidth="20dp"
            android:singleLine="true"
            android:textColor="@color/t333333"
            android:textColorHint="@color/hintB5"
            android:textSize="12sp" />

        <LinearLayout
            android:id="@+id/login_linear_deletpass"
            android:layout_width="@dimen/d_36dp"
            android:layout_height="@dimen/d_36dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="2dp"
            android:gravity="center"
            android:visibility="gone" >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/login_delete" />
        </LinearLayout>
    </RelativeLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="0.5dp"
        android:background="@color/adadad" />

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="@dimen/d_48dp"
        android:background="@color/ffffff" >


        <TextView
            android:id="@+id/login_img_locked1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:text="确认登录密码"
            android:textColor="@color/text292929"
            android:textSize="17sp" />

        <EditText
            android:id="@+id/login_edit_pass2"
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_toRightOf="@id/login_img_locked1"
            android:background="@color/ffffff"
            android:digits="0123456789qwertyuioplkjhgfdsazxcvbnm"
            android:hint="请再次输入密码"
            android:inputType="textPassword"
            android:maxWidth="20dp"
            android:singleLine="true"
            android:textColor="@color/t333333"
            android:textColorHint="@color/hintB5"
            android:textSize="12sp" />

        <LinearLayout
            android:id="@+id/login_linear_deletpass2"
            android:layout_width="@dimen/d_36dp"
            android:layout_height="@dimen/d_36dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="2dp"
            android:gravity="center"
            android:visibility="gone" >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/login_delete" />
        </LinearLayout>
    </RelativeLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="0.5dp"
        android:background="@color/adadad" />

    <LinearLayout
        android:visibility="gone"
        android:id="@+id/msgLayout"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:layout_marginTop="10dp"
        android:orientation="horizontal" >

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/orange_error" />

        <TextView
            android:id="@+id/register_hint"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:gravity="center"
            android:text="您填写的手机号已经注册"
            android:textColor="@color/hint6C"
            android:textSize="@dimen/s_13sp" />
    </LinearLayout>
    
    <LinearLayout
        android:id="@+id/login_linear_signin"
        android:layout_width="wrap_content"
        android:layout_height="44dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="30dp"
        android:background="@drawable/bg_blue_shape"
        android:gravity="center"
        android:orientation="horizontal" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="60dp"
            android:layout_marginRight="60dp"
            android:text="提交"
            android:textColor="@color/fffefe"
            android:textSize="@dimen/s_17sp" />
    </LinearLayout>

